def yuki_selfies():
    yuki_pic = ['test 1', 'test 2', 'test 3']
    return yuki_pic

def yuki_answers():
    answers = ['My doggo senses is telling me yes',
               'My doggo senses is saying it will definitely happen!',
               'Yuki says yes!'
               'My doggo senses is telling me no',
               'Yuki says no',
               "Unfortunately that will never happen. It's not my senses, I just don't think it will work",
               'Ehhh...maybe? Bork',
               "I don't know, I'm just a dog ;-;",
               '¯\_(ツ)_/¯',
               'Bork!, try asking again']
    return answers
